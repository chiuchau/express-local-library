extends layout

block content
  h1 #{title}: #{bookinstance.book.title} - #{bookinstance.imprint}
  
  p Do you really want to delete this book instance?
  
  form(method='POST' action='')
    div.form-group
      input#bookinstance_id.form-control(type='hidden',name='bookinstance_id', required='true', value=bookinstance._id )

    button.btn.btn-primary(type='submit') Delete